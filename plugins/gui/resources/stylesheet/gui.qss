QLineEdit {
    background-color : rgba(43, 43, 43, 1);
    color            : white;
    border           : 1px solid rgba(128, 128, 128, 1);
    /*border: 3px solid rgba(43, 43, 43, 1);*/
}

QPlainTextEdit {
    border : none;
}

QTreeView {
    alternate-background-color : rgba(48, 49, 49, 1);
    background-color           : rgba(43, 43, 43, 1);
    border                     : none;
}

