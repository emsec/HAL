QTabWidget {
    background   : rgba(43, 43, 43, 1);
    border-width : 0px;
}

QTabWidget::tab-bar {
    alignment : left;
}

QTabBar {
    alignment : left;
    border    : none;
    /*  border-bottom: 5px solid rgba(81, 86, 88, 1); */
}

QTabBar::tab {
    background    : rgba(60, 62, 63, 1);
    /*border: 1px solid rgba(40, 40, 40, 1);*/
    border-bottom : none;
    min-width     : 8ex;
    padding       : 5px;
    /*  margin-bottom: 5px; */
}

QTabBar::tab:selected {
    background          : rgba(81, 86, 88, 1);
    border-color        : rgba(40, 40, 40, 1);
    border-bottom-color : rgba(81, 86, 88, 1);
    color               : rgba(228, 228, 228, 1);
}

QTabBar::tab:movable {
    color : rgba(228, 228, 228, 1);
}

QTabBar::tear {
    width  : 0px;
    border : none;
}

QGridLayout {
    margin  : 0px;
    padding : 0px;
}

QMainWindow::separator {
    background : yellow;
    width      : 100px; /* when vertical */
    height     : 100px; /* when horizontal */
}

QTableView {
    alternate-background-color : rgba(48, 49, 49, 1);
    background-color           : rgba(43, 43, 43, 1);
    border                     : none;
}

QWidget {
    background-color : rgba(60, 63, 65, 1);
}

QTreeView {
    background-color : rgba(60, 63, 65, 1);
}

